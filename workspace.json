{
	"version": 2,
	"projects": {
		"all": {
			"root": "packages/",
			"projectType": "library",
			"targets": {
				"build": {
					"executor": "@nrwl/workspace:run-commands",
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx run nativescript-downloader:build.all"],
						"parallel": false
					}
				},
				"focus": {
					"outputs": ["dist/packages"],
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages"],
						"parallel": false
					},
					"executor": "@nrwl/workspace:run-commands"
				}
			}
		},
		"demo": {
			"root": "apps/demo/",
			"sourceRoot": "apps/demo/src",
			"projectType": "application",
			"prefix": "demo",
			"targets": {
				"build": {
					"executor": "@nativescript/nx:build",
					"options": {
						"noHmr": true,
						"production": true,
						"uglify": true,
						"release": true,
						"forDevice": true
					}
				},
				"ios": {
					"executor": "@nativescript/nx:build",
					"options": {
						"platform": "ios"
					}
				},
				"android": {
					"executor": "@nativescript/nx:build",
					"options": {
						"platform": "android"
					}
				},
				"clean": {
					"executor": "@nativescript/nx:build",
					"options": {
						"clean": true
					}
				}
			}
		},
		"demo-angular": {
			"root": "apps/demo-angular/",
			"sourceRoot": "apps/demo-angular/src",
			"projectType": "application",
			"prefix": "demo",
			"targets": {
				"build": {
					"executor": "@nativescript/nx:build",
					"options": {
						"noHmr": true,
						"production": true,
						"uglify": true,
						"release": true,
						"forDevice": true
					}
				},
				"ios": {
					"executor": "@nativescript/nx:build",
					"options": {
						"platform": "ios"
					}
				},
				"android": {
					"executor": "@nativescript/nx:build",
					"options": {
						"platform": "android"
					}
				},
				"clean": {
					"executor": "@nativescript/nx:build",
					"options": {
						"clean": true
					}
				}
			}
		},
		"nativescript-downloader": {
			"root": "packages/nativescript-downloader",
			"projectType": "library",
			"sourceRoot": "packages/nativescript-downloader",
			"targets": {
				"build": {
					"executor": "@nrwl/node:package",
					"options": {
						"outputPath": "dist/packages/nativescript-downloader",
						"tsConfig": "packages/nativescript-downloader/tsconfig.json",
						"packageJson": "packages/nativescript-downloader/package.json",
						"main": "packages/nativescript-downloader/index.d.ts",
						"assets": [
							"packages/nativescript-downloader/*.md",
							"packages/nativescript-downloader/index.d.ts",
							"LICENSE",
							{
								"glob": "**/*",
								"input": "packages/nativescript-downloader/platforms/",
								"output": "./platforms/"
							}
						]
					}
				},
				"build.all": {
					"executor": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["nx run nativescript-downloader:build", "node tools/scripts/build-finish.ts nativescript-downloader"],
						"parallel": false
					}
				},
				"focus": {
					"executor": "@nrwl/workspace:run-commands",
					"options": {
						"commands": ["nx g @nativescript/plugin-tools:focus-packages nativescript-downloader"],
						"parallel": false
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/workspace"
	}
}
